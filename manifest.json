{
  "manifest_version": 2,
  "name": "Papatabs",
  "version": "0.1",
  "description": "Just another tab and session manager extension",
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/browser-polyfill.js",
      "js/dexie.js",
      "js/papatabs.js",
      "js/background.js"
    ]
  },
  "icons": {
    "16": "./icon16.png",
    "64": "./icon64.png",
    "128": "./icon128.png"
  },
  "browser_action": {
    "default_title": "Open Papatab overview"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Home"
      }
    },
    "switch-next-window": {
      "suggested_key": {
        "default": "Ctrl+Shift+PageUp"
      },
      "description": "Switch to the last window."
    },
    "switch-last-window": {
      "suggested_key": {
        "default": "Ctrl+Shift+PageDown"
      },
      "description": "Switch to the next window."
    }
  },
  "optional_permissions": [ "bookmarks", "contextMenus" ],
  "permissions": [ "tabs", "sessions", "storage" ],
  "minimum_chrome_version": "71"
}